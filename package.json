{
  "name": "@tapico/msw-webarchive",
  "version": "1.0.1",
  "description": "An utility to drive requests handlers through a `.har` web-archive file",
  "main": "dist/umd/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/umd/index.js",
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "node",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development rollup -c rollup.config.ts -w",
    "clean": "rimraf dist",
    "lint": "eslint \"src/**/*.ts\"",
    "build": "yarn clean && cross-env NODE_ENV=production rollup -c rollup.config.ts",
    "test": "yarn test:unit",
    "test:unit": "cross-env BABEL_ENV=test jest --runInBand",
    "prepublishOnly": "yarn test:unit && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tapico/tapico-msw-webarchive.git"
  },
  "keywords": [
    "msw",
    "mock",
    ".har",
    "devtools"
  ],
  "author": {
    "name": "Weyert de Boer"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint"
    ]
  },
  "bugs": {
    "url": "https://github.com/Tapico/tapico-msw-webarchive/issues"
  },
  "homepage": "https://github.com/Tapico/tapico-msw-webarchive#readme",
  "sideEffects": false,
  "peerDependencies": {
    "msw": ">=2.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-inject": "^5.0.5",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@types/jest": "^29.5.12",
    "@types/node-fetch": "^2",
    "@types/set-cookie-parser": "^2.4.7",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^11.2.6",
    "msw": "^2.2.13",
    "node-fetch": "^2.6.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.5",
    "rollup": "^2.79.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.3",
    "undici": "^6.10.2",
    "web-streams-polyfill": "^4.0.0",
    "release-please": "^16.10.1"
  }
}
